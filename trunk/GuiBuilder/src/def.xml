<Definition>

<Types>

	<Type name="Boolean" editor="org.aswing.guibuilder.property.BooleanEditor">
	</Type>

	<Type name="int" editor="org.aswing.guibuilder.property.IntEditor">
	</Type>

	<Type name="String" editor="org.aswing.guibuilder.property.StringEditor">
