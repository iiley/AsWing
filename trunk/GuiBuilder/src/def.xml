	</Type>

	<Type name="IntPoint" editor="org.aswing.guibuilder.property.IntPointEditor">
	</Type>

	<Type name="PreferSize" editor="org.aswing.guibuilder.property.PreferSizeEditor">
	</Type>

	<Type name="Align" editor="org.aswing.guibuilder.property.AlignEditor">
	</Type>

	<Type name="Layout" editor="org.aswing.guibuilder.property.LayoutEditor">
	</Type>

</Types>

<Layouts>
	<Layout name="EmptyLayout" clazz="org.aswing.EmptyLayout">
	</Layout>
	<Layout name="FlowLayout" clazz="org.aswing.FlowLayout">
		<Property name="align" property="Alignment" type="Align">
		</Property>
		<Property name="hgap" property="Hgap" type="int">
		</Property>
		<Property name="vgap" property="Vgap" type="int">
		</Property>
		<Property name="margin" property="Margin" type="Boolean">
		</Property>
	</Layout>
	<Layout name="BorderLayout" clazz="org.aswing.BorderLayout">
		<Property name="hgap" property="Hgap" type="int">
		</Property>
		<Property name="vgap" property="Vgap" type="int">
		</Property>
	</Layout>
	
</Layouts>

<Components>
	<BaseComponent name="Component" clazz="org.aswing.Component">
		<Property name="ID" property="id" type="String">
		</Property>
		<Property name="Opaque" property="Opaque" type="Boolean" action="repaint">
		</Property>
		<Property name="Location" property="Location" type="IntPoint" action="revalidate">
		</Property>
		<Property name="Size" property="Size" type="IntDimension" defaultValue="40,40" action="revalidate">
		</Property>
		<Property name="PreferSize" property="PreferredSize" type="PreferSize" action="revalidate">
		</Property>
		<Property name="constraints" property="Constraints" type="String" action="reAppendToParent">
		</Property>
	</BaseComponent>
	
	<BaseContainer name="Container" clazz="org.aswing.Container" sup="Component">
